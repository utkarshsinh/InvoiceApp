{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapMutations, mapState, mapActions } from \"vuex\";\nexport default {\n  name: \"invoiceView\",\n  data() {\n    return {\n      currentInvoice: null\n    };\n  },\n  created() {\n    this.getCurrentInvoice();\n  },\n  methods: {\n    ...mapMutations([\"SET_CURRENT_INVOICE\", \"TOGGLE_EDIT_INVOICE\", \"TOGGLE_INVOICE\"]),\n    ...mapActions([\"DELETE_INVOICE\", \"UPDATE_STATUS_TO_PENDING\", \"UPDATE_STATUS_TO_PAID\"]),\n    getCurrentInvoice() {\n      this.SET_CURRENT_INVOICE(this.$route.params.invoiceId);\n      this.currentInvoice = this.currentInvoiceArray[0];\n    },\n    toggleEditInvoice() {\n      this.TOGGLE_EDIT_INVOICE();\n      this.TOGGLE_INVOICE();\n    },\n    async deleteInvoice(docId) {\n      await this.DELETE_INVOICE(docId);\n      this.$router.push({\n        name: \"HomePage\"\n      });\n    },\n    updateStatusToPaid(docId) {\n      this.UPDATE_STATUS_TO_PAID(docId);\n    },\n    updateStatusToPending(docId) {\n      this.UPDATE_STATUS_TO_PENDING(docId);\n    }\n  },\n  computed: {\n    ...mapState(['currentInvoiceArray', \"editInvoice\"])\n  },\n  watch: {\n    editInvoice() {\n      if (!this.editInvoice) {\n        this.currentInvoice = this.currentInvoiceArray[0];\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapMutations","mapState","mapActions","name","data","currentInvoice","created","getCurrentInvoice","methods","SET_CURRENT_INVOICE","$route","params","invoiceId","currentInvoiceArray","toggleEditInvoice","TOGGLE_EDIT_INVOICE","TOGGLE_INVOICE","deleteInvoice","docId","DELETE_INVOICE","$router","push","updateStatusToPaid","UPDATE_STATUS_TO_PAID","updateStatusToPending","UPDATE_STATUS_TO_PENDING","computed","watch","editInvoice"],"sources":["/Users/utkarshsinh/Desktop/Projects/Invoice/typescriptreact-invoice-app-fmgtix/invoice/src/views/InvoiceView.vue"],"sourcesContent":["<template>\n  <div v-if=\"currentInvoice\" class=\"invoice-view container\">\n      <router-link class=\"nav-link flex\" :to=\"{ name: 'HomePage' }\">\n          <img src=\"@/assets/icon-arrow-left.svg\" alt=\"\" /> Go Back\n      </router-link>\n      <!-- Header -->\n      <div class=\"header flex\">\n          <div class=\"left flex\">\n              <span>Status</span>\n              <div class=\"status-button flex\" :class=\"{ paid: currentInvoice.invoicePaid, draft: currentInvoice.invoiceDraft, pending: currentInvoice.invoicePending }\">\n                <span v-if=\"currentInvoice.invoicePaid\">Paid</span>\n                <span v-if=\"currentInvoice.invoiceDraft\">Draft</span>\n                <span v-if=\"currentInvoice.invoicePending\">Pending</span>\n              </div>\n          </div>\n          <div class=\"right flex\">\n            <button @click=\"toggleEditInvoice\" class=\"dark-purple\">Edit</button>\n            <button @click=\"deleteInvoice(currentInvoice.docId)\" class=\"red\">Delete</button>\n            <button @click=\"updateStatusToPaid(currentInvoice.docId)\" v-if=\"currentInvoice.invoicePending\" class=\"green\">Mark as Paid</button>\n            <button v-if=\"currentInvoice.invoiceDraft || currentInvoice.invoicePaid\" @click=\"updateStatusToPending(currentInvoice.docId)\" class=\"orange\">Mark as Pending</button>\n          </div>\n      </div>\n\n      <!-- Invoice Detail -->\n      <div class=\"invoice-details flex flex-column\">\n      <div class=\"top flex\">\n        <div class=\"left flex flex-column\">\n          <p><span>#</span>{{ currentInvoice.invoiceId }}</p>\n          <p>{{ currentInvoice.productDescription }}</p>\n        </div>\n        <div class=\"right flex flex-column\">\n          <p>{{ currentInvoice.billerStreetAddress }}</p>\n          <p>{{ currentInvoice.billerCity }}</p>\n          <p>{{ currentInvoice.billerZipCode }}</p>\n          <p>{{ currentInvoice.billerCountry }}</p>\n        </div>\n      </div>\n      <div class=\"middle flex\">\n        <div class=\"payment flex flex-column\">\n          <h4>Invoice Date</h4>\n          <p>\n            {{ currentInvoice.invoiceDate }}\n          </p>\n          <h4>Payment Date</h4>\n          <p>\n            {{ currentInvoice.paymentDueDate }}\n          </p>\n        </div>\n        <div class=\"bill flex flex-column\">\n          <h4>Bill To</h4>\n          <p>{{ currentInvoice.clientName }}</p>\n          <p>{{ currentInvoice.clientStreetAddress }}</p>\n          <p>{{ currentInvoice.clientCity }}</p>\n          <p>{{ currentInvoice.clientZipCode }}</p>\n          <p>{{ currentInvoice.clientCountry }}</p>\n        </div>\n        <div class=\"send-to flex flex-column\">\n          <h4>Sent To</h4>\n          <p>{{ currentInvoice.clientEmail }}</p>\n        </div>\n      </div>\n      <div class=\"bottom flex flex-column\">\n        <div class=\"billing-items\">\n          <div class=\"heading flex\">\n            <p>Item Name</p>\n            <p>QTY</p>\n            <p>Price</p>\n            <p>Total</p>\n          </div>\n          <div v-for=\"(item, index) in currentInvoice.invoiceItemList\" :key=\"index\" class=\"item flex\">\n            <p>${{ item.itemName }}</p>\n            <p>${{ item.qty }}</p>\n            <p>${{ item.price }}</p>\n            <p>${{ item.total }}</p>\n          </div>\n        </div>\n        <div class=\"total flex\">\n          <p>Amount Due</p>\n          <p>${{ currentInvoice.invoiceTotal }}</p>\n        </div>\n      </div>\n    </div>\n\n\n  </div>\n</template>\n\n<script>\nimport { mapMutations, mapState, mapActions} from \"vuex\";\nexport default {\n    name: \"invoiceView\",\n    data() {\n        return {\n        currentInvoice: null,\n        };\n    },\n    created() {\n        this.getCurrentInvoice();\n    },\n    methods: {\n        ...mapMutations([\"SET_CURRENT_INVOICE\", \"TOGGLE_EDIT_INVOICE\", \"TOGGLE_INVOICE\"]),\n\n\n        ...mapActions([\"DELETE_INVOICE\", \"UPDATE_STATUS_TO_PENDING\", \"UPDATE_STATUS_TO_PAID\"]),\n\n        getCurrentInvoice() {\n        this.SET_CURRENT_INVOICE(this.$route.params.invoiceId);\n        this.currentInvoice = this.currentInvoiceArray[0];\n        },\n\n        toggleEditInvoice() {\n        this.TOGGLE_EDIT_INVOICE();\n        this.TOGGLE_INVOICE();\n        },\n\n        async deleteInvoice(docId) {\n            \n        await this.DELETE_INVOICE(docId);\n        this.$router.push({ name: \"HomePage\" });\n        },\n\n        updateStatusToPaid(docId) {\n        this.UPDATE_STATUS_TO_PAID(docId);\n        },\n\n        updateStatusToPending(docId) {\n        this.UPDATE_STATUS_TO_PENDING(docId);\n        },\n\n\n    },\n    computed: {\n        ...mapState(['currentInvoiceArray', \"editInvoice\"])\n    },\n    watch: {\n    editInvoice() {\n      if (!this.editInvoice) {\n        this.currentInvoice = this.currentInvoiceArray[0];\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.invoice-view {\n  .nav-link {\n    margin-bottom: 32px;\n    align-items: center;\n    color: #fff;\n    font-size: 12px;\n    img {\n      margin-right: 16px;\n      width: 7px;\n      height: 10px;\n    }\n  }\n\n  .header,\n  .invoice-details {\n    background-color: #1e2139;\n    border-radius: 20px;\n  }\n\n  .header {\n    align-items: center;\n    padding: 24px 32px;\n    font-size: 12px;\n\n    .left {\n      align-items: center;\n\n      span {\n        color: #dfe3fa;\n        margin-right: 16px;\n      }\n    }\n\n    .right {\n      flex: 1;\n      justify-content: flex-end;\n\n      button {\n        color: #fff;\n      }\n    }\n  }\n\n  .invoice-details {\n    padding: 48px;\n    margin-top: 24px;\n\n    .top {\n      div {\n        color: #dfe3fa;\n        flex: 1;\n      }\n\n      .left {\n        font-size: 12px;\n        p:first-child {\n          font-size: 24px;\n          text-transform: uppercase;\n          color: #fff;\n          margin-bottom: 8px;\n        }\n\n        p:nth-child(2) {\n          font-size: 16px;\n        }\n\n        span {\n          color: #888eb0;\n        }\n      }\n\n      .right {\n        font-size: 12px;\n        align-items: flex-end;\n      }\n    }\n\n    .middle {\n      margin-top: 50px;\n      color: #dfe3fa;\n      gap: 16px;\n\n      h4 {\n        font-size: 12px;\n        font-weight: 400;\n        margin-bottom: 12px;\n      }\n\n      p {\n        font-size: 16px;\n      }\n\n      .bill,\n      .payment {\n        flex: 1;\n      }\n\n      .payment {\n        h4:nth-child(3) {\n          margin-top: 32px;\n        }\n\n        p {\n          font-weight: 600;\n        }\n      }\n\n      .bill {\n        p:nth-child(2) {\n          font-size: 16px;\n        }\n        p:nth-child(3) {\n          margin-top: auto;\n        }\n\n        p {\n          font-size: 12px;\n        }\n      }\n\n      .send-to {\n        flex: 2;\n      }\n    }\n\n    .bottom {\n      margin-top: 50px;\n\n      .billing-items {\n        padding: 32px;\n        border-radius: 20px 20px 0 0;\n        background-color: #252945;\n\n        .heading {\n          color: #dfe3fa;\n          font-size: 12px;\n          margin-bottom: 32px;\n\n          p:first-child {\n            flex: 3;\n            text-align: left;\n          }\n\n          p {\n            flex: 1;\n            text-align: right;\n          }\n        }\n\n        .item {\n          margin-bottom: 32px;\n          font-size: 13px;\n          color: #fff;\n\n          &:last-child {\n            margin-bottom: 0;\n          }\n\n          p:first-child {\n            flex: 3;\n            text-align: left;\n          }\n\n          p {\n            flex: 1;\n            text-align: right;\n          }\n        }\n      }\n\n      .total {\n        color: #fff;\n        padding: 32px;\n        background-color: rgba(12, 14, 22, 0.7);\n        align-items: center;\n        border-radius: 0 0 20px 20px;\n\n        p {\n          flex: 1;\n          font-size: 12px;\n        }\n\n        p:nth-child(2) {\n          font-size: 28px;\n          text-align: right;\n        }\n      }\n    }\n  }\n}\n</style>"],"mappings":";AAwFA,SAASA,YAAY,EAAEC,QAAQ,EAAEC,UAAU,QAAO,MAAM;AACxD,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACPC,cAAc,EAAE;IAChB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACL,GAAGR,YAAY,CAAC,CAAC,qBAAqB,EAAE,qBAAqB,EAAE,gBAAgB,CAAC,CAAC;IAGjF,GAAGE,UAAU,CAAC,CAAC,gBAAgB,EAAE,0BAA0B,EAAE,uBAAuB,CAAC,CAAC;IAEtFK,iBAAiBA,CAAA,EAAG;MACpB,IAAI,CAACE,mBAAmB,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,SAAS,CAAC;MACtD,IAAI,CAACP,cAAa,GAAI,IAAI,CAACQ,mBAAmB,CAAC,CAAC,CAAC;IACjD,CAAC;IAEDC,iBAAiBA,CAAA,EAAG;MACpB,IAAI,CAACC,mBAAmB,CAAC,CAAC;MAC1B,IAAI,CAACC,cAAc,CAAC,CAAC;IACrB,CAAC;IAED,MAAMC,aAAaA,CAACC,KAAK,EAAE;MAE3B,MAAM,IAAI,CAACC,cAAc,CAACD,KAAK,CAAC;MAChC,IAAI,CAACE,OAAO,CAACC,IAAI,CAAC;QAAElB,IAAI,EAAE;MAAW,CAAC,CAAC;IACvC,CAAC;IAEDmB,kBAAkBA,CAACJ,KAAK,EAAE;MAC1B,IAAI,CAACK,qBAAqB,CAACL,KAAK,CAAC;IACjC,CAAC;IAEDM,qBAAqBA,CAACN,KAAK,EAAE;MAC7B,IAAI,CAACO,wBAAwB,CAACP,KAAK,CAAC;IACpC;EAGJ,CAAC;EACDQ,QAAQ,EAAE;IACN,GAAGzB,QAAQ,CAAC,CAAC,qBAAqB,EAAE,aAAa,CAAC;EACtD,CAAC;EACD0B,KAAK,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAAC,IAAI,CAACA,WAAW,EAAE;QACrB,IAAI,CAACvB,cAAa,GAAI,IAAI,CAACQ,mBAAmB,CAAC,CAAC,CAAC;MACnD;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}