{"ast":null,"code":"import { mapState, mapActions } from \"vuex\";\nimport InvoiceModalVue from './components/InvoiceModal.vue';\nimport AppNavigation from \"./components/Navigation\";\n// import ModalPopup from \"./components/Modal\"\nexport default {\n  data() {\n    return {\n      mobile: null\n    };\n  },\n  components: {\n    AppNavigation,\n    InvoiceModalVue\n    // ModalPopup\n  },\n  created() {\n    this.GET_INVOICES();\n    this.checkScreen();\n    window.addEventListener(\"resize\", this.checkScreen);\n  },\n  methods: {\n    ...mapActions(['GET_INVOICES']),\n    checkScreen() {\n      const windowWidth = window.innerWidth;\n      if (windowWidth <= 750) {\n        this.mobile = true;\n        return;\n      }\n      this.mobile = false;\n    }\n  },\n  computed: {\n    ...mapState(['invoiceModal', 'modalActive', 'invoicesLoaded'])\n  }\n};","map":{"version":3,"names":["mapState","mapActions","InvoiceModalVue","AppNavigation","data","mobile","components","created","GET_INVOICES","checkScreen","window","addEventListener","methods","windowWidth","innerWidth","computed"],"sources":["/Users/utkarshsinh/Desktop/Projects/Invoice/typescriptreact-invoice-app-fmgtix/invoice/src/App.vue"],"sourcesContent":["<template>\n  <div v-if=\"invoicesLoaded\">\n    <div v-if=\"!mobile\" class=\"app flex flex-column\">\n      <AppNavigation />\n      <div class=\"app-content flex flex-column\">\n        <!-- <ModalPopup v-if=\"modalActive\"/> -->\n        <InvoiceModalVue v-if=\"invoiceModal\" />\n      <router-view />\n      </div>\n    </div>\n    <div v-else class=\"mobile-message flex flex-column\">\n      <h2>Sorry, this app is not supported on Mobile Devices</h2>\n      <p>To use this app, please use a computer or Tablet</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions } from \"vuex\";\nimport InvoiceModalVue from './components/InvoiceModal.vue';\nimport AppNavigation from \"./components/Navigation\"\n// import ModalPopup from \"./components/Modal\"\nexport default {\n  data() {\n    return {\n      mobile: null,\n    };\n  },\n  components: {\n    AppNavigation,\n    InvoiceModalVue,\n    // ModalPopup\n  },\n  created() {\n    this.GET_INVOICES();\n    this.checkScreen();\n    window.addEventListener(\"resize\", this.checkScreen);\n  },\n  methods: {\n\n    ...mapActions(['GET_INVOICES']),\n    checkScreen() {\n      const windowWidth = window.innerWidth;\n      if (windowWidth <= 750) {\n        this.mobile = true;\n        return;\n      }\n      this.mobile = false;\n    },\n  },\n  computed: {\n    ...mapState(['invoiceModal', 'modalActive', 'invoicesLoaded'])\n  }\n};\n</script>\n\n\n<style lang=\"scss\">\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600&display=swap\");\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: \"Poppins\", sans-serif;\n\n}\n.app {\n  background-color: #141625;\n  min-height: 100vh;\n  @media (min-width: 900px) {\n    flex-direction: row !important;\n  }\n  .app-content {\n    padding: 0 20px;\n    flex: 1;\n    position: relative;\n  }\n}\n\n.mobile-message {\n  text-align: center;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n  background-color: #141625;\n  color: #fff;\n\n  p {\n    margin-top: 16px;\n  }\n}\n\n\nbutton,\n.button {\n  cursor: pointer;\n  padding: 16px 24px;\n  border-radius: 30px;\n  border: none;\n  font-size: 12px;\n  margin-right: 8px;\n  color: #fff;\n}\n\n.dark-purple {\n  background-color: #252945;\n}\n\n.red {\n  background-color: #ec5757;\n}\n\n.purple {\n  background-color: #7c5dfa;\n}\n\n.green {\n  background-color: #33d69f;\n}\n\n.orange {\n  background-color: #ff8f00;\n}\n\n// utility classes\n\n.flex {\n  display: flex;\n}\n\n.flex-column {\n  flex-direction: column;\n}\n\n.container {\n  width: 100%;\n  padding: 40px 10px;\n  max-width: 850px;\n  margin: 0 auto;\n\n  @media (min-width: 900px) {\n    padding-top: 72px;\n  }\n}\n\n.nav-link {\n  text-decoration: none;\n  color: initial;\n}\n\n// Status Button Styling\n\n.status-button {\n  &::before {\n    content: \"\";\n    width: 10px;\n    height: 10px;\n    border-radius: 50%;\n    margin-right: 8px;\n  }\n  font-size: 12px;\n  margin-right: 30px;\n  align-items: center;\n  padding: 8px 30px;\n  border-radius: 10px;\n}\n\n.paid {\n  &::before {\n    background-color: #33d69f;\n  }\n  color: #33d69f;\n  background-color: rgba(51, 214, 160, 0.1);\n}\n\n.pending {\n  &::before {\n    background-color: #ff8f00;\n  }\n  color: #ff8f00;\n  background-color: rgba(255, 145, 0, 0.1);\n}\n\n.draft {\n  &::before {\n    background-color: #dfe3fa;\n  }\n  color: #dfe3fa;\n  background-color: rgba(223, 227, 250, 0.1);\n}\n</style>"],"mappings":"AAkBA,SAASA,QAAQ,EAAEC,UAAS,QAAS,MAAM;AAC3C,OAAOC,eAAc,MAAO,+BAA+B;AAC3D,OAAOC,aAAY,MAAO,yBAAwB;AAClD;AACA,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVH,aAAa;IACbD;IACA;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;IACnB,IAAI,CAACC,WAAW,CAAC,CAAC;IAClBC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACF,WAAW,CAAC;EACrD,CAAC;EACDG,OAAO,EAAE;IAEP,GAAGX,UAAU,CAAC,CAAC,cAAc,CAAC,CAAC;IAC/BQ,WAAWA,CAAA,EAAG;MACZ,MAAMI,WAAU,GAAIH,MAAM,CAACI,UAAU;MACrC,IAAID,WAAU,IAAK,GAAG,EAAE;QACtB,IAAI,CAACR,MAAK,GAAI,IAAI;QAClB;MACF;MACA,IAAI,CAACA,MAAK,GAAI,KAAK;IACrB;EACF,CAAC;EACDU,QAAQ,EAAE;IACR,GAAGf,QAAQ,CAAC,CAAC,cAAc,EAAE,aAAa,EAAE,gBAAgB,CAAC;EAC/D;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}